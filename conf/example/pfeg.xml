<datasets>
  <pfeg
    name = "NOAA Time Series (Example of data from a Dapper Server)"
    url = "http://las.pfeg.noaa.gov/dods/ndbc/all_noaa_time_series.cdp"
    doc = "">

    <properties>
    <ui>
      <default>file:ui.xml#dapper_demo</default>
    </ui>
       <database_access>        
           <positive>down</positive>
           <time>location.profile.TIME</time>
           <depth_units>meters</depth_units>
           <db_table>all_noaa_time_series.cdp</db_table>
           <depth>location.DEPTH</depth>
           <db_title>NOAA Time Series</db_title>
           <missing>NaN</missing>
           <lon_domain>0:360</lon_domain>
           <time_units>msec since 1970-01-01 00:00:00</time_units>
           <longitude>location.LON</longitude>
           <db_name>NOAA Time Series</db_name>
           <db_server>PFEG Server</db_server>
           <time_type>double</time_type>
           <db_type>dapper</db_type>
           <latitude>location.LAT</latitude>
       </database_access>
       <product_server>
           <ui_timeout>1000</ui_timeout>
           <ps_timeout>3600</ps_timeout>
           <use_cache>false</use_cache>
       </product_server>
     </properties>

    <variables>
      <pfeg_mwd1 units="degrees_true" name="mean wave direction" url="#MWD1">
        <link match="/lasdata/grids/pfeg_grid"/>
      </pfeg_mwd1>
      <pfeg_WVHT1 units="meters" name="wave height" url="#WVHT1">
        <link match="/lasdata/grids/pfeg_grid"/>
      </pfeg_WVHT1>
      <pfeg_WSPV1 units="WSPV1" name="merid. wind speed" url="#WSPV1">
        <link match="/lasdata/grids/pfeg_grid"/>
      </pfeg_WSPV1>
      <pfeg_WTMP1 units="degree_c" name="sst" url="#WTMP1">
        <link match="/lasdata/grids/pfeg_grid"/>
      </pfeg_WTMP1>
      <pfeg_TIDE1 units="m" name="water level" url="#TIDE1">
        <link match="/lasdata/grids/pfeg_grid"/>
      </pfeg_TIDE1>
      <pfeg_WD1 units="degrees_true" name="wind direction" url="#WD1">
        <link match="/lasdata/grids/pfeg_grid"/>
      </pfeg_WD1>
      <pfeg_APD1 units="seconds" name="average wave period" url="#APD1">
        <link match="/lasdata/grids/pfeg_grid"/>
      </pfeg_APD1>
      <pfeg_PTDY1 units="hpa" name="pressure tendency" url="#PTDY1">
        <link match="/lasdata/grids/pfeg_grid"/>
      </pfeg_PTDY1>
      <pfeg_GST1 units="m/sec" name="gust speed" url="#GST1">
        <link match="/lasdata/grids/pfeg_grid"/>
      </pfeg_GST1>
      <pfeg_VIS1 units="km" name="station visibility" url="#VIS1">
        <link match="/lasdata/grids/pfeg_grid"/>
      </pfeg_VIS1>
      <pfeg_DPD1 units="seconds" name="dominant wave period" url="#DPD1">
        <link match="/lasdata/grids/pfeg_grid"/>
      </pfeg_DPD1>
      <pfeg_ATMP1 units="degree_c" name="air temperature" url="#ATMP1">
        <link match="/lasdata/grids/pfeg_grid"/>
      </pfeg_ATMP1>
      <pfeg_DEWP1 units="degree_c" name="dewpoint temperature" url="#DEWP1">
        <link match="/lasdata/grids/pfeg_grid"/>
      </pfeg_DEWP1>
      <pfeg_WSPU1 units="m/s" name="zonal wind speed" url="#WSPU1">
        <link match="/lasdata/grids/pfeg_grid"/>
      </pfeg_WSPU1>
      <pfeg_WSPD1 units="m/s" name="wind speed" url="#WSPD1">
        <link match="/lasdata/grids/pfeg_grid"/>
      </pfeg_WSPD1>
      <pfeg_BAR1 units="hpa" name="sea level pressure" url="#BAR1">
        <link match="/lasdata/grids/pfeg_grid"/>
      </pfeg_BAR1>
    </variables>
  </pfeg>
</datasets>

<grids>
  <pfeg_grid>
    <link match="/lasdata/axes/pfeg_x"/>
    <link match="/lasdata/axes/pfeg_y"/>
    <link match="/lasdata/axes/pfeg_time"/>
  </pfeg_grid>
</grids>

<axes>
  <pfeg_y units="degrees_north" type="y">
    <arange start="32.43" step="1" size="10"/>
  </pfeg_y>
  <pfeg_x units="degrees_east" type="x">
    <arange start="230" step="1" size="10"/>
  </pfeg_x>
  <pfeg_time units="hour" type="t">
   <arange start="1982-01-18" step="1" size="212335"/>
  </pfeg_time>
</axes>
